{
  "name": "ai-sdlc-framework",
  "version": "3.3.2",
  "description": "Universal AI-SDLC Framework for any technology stack - AI-powered development automation with quality gates, testing, and intelligent PR reviews",
  "main": "index.js",
  "scripts": {
    "dev": "vite dev",
    "build": "node -e \"const fs=require('fs');['dist','build','public'].forEach(d=>fs.mkdirSync(d,{recursive:true})); fs.writeFileSync('dist/index.txt','AI-SDLC demo build artifact');\"",
    "test": "vitest run --no-watch",
    "test:ci": "vitest run --no-watch --reporter=verbose tests/unit",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage --no-watch",
    "test:coverage:sonar": "vitest run --coverage --no-watch --reporter=verbose",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ci": "eslint .",
    "lint:ci:all": "eslint .",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx,json,md}'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,json,md}'",
    "format:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,md}'",
    "type-check": "tsc --noEmit",
    "quality-gates": "./scripts/local-quality-gates.sh",
    "pre-push": "npm run quality-gates"
  },
  "keywords": [
    "ai-sdlc",
    "development-automation",
    "quality-gates",
    "ai-testing",
    "pr-agent",
    "multi-stack",
    "universal-framework",
    "development-tools",
    "automation",
    "testing",
    "code-quality"
  ],
  "author": "The Credit Pros Development Team",
  "license": "MIT",
  "overrides": {
    "debug": "^4.3.4",
    "cross-spawn": "^7.0.6",
    "esbuild": "^0.25.9",
    "@babel/core": "^7.26.0",
    "@babel/traverse": "^7.26.0",
    "@babel/helper-module-transforms": "^7.26.0",
    "@babel/helper-module-imports": "^7.26.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@playwright/test": "^1.55.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "24.3.1",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "5.0.2",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "axios": "1.11.0",
    "commitlint": "^19.8.1",
    "eslint": "^9.35.0",
    "husky": "^9.1.7",
    "jsdom": "^16.5.3",
    "lint-staged": "^15.2.10",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "5.9.2",
    "vite": "^6.0.0",
    "vitest": "^0.34.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TheCreditPros/dev_framework_demo.git"
  },
  "bugs": {
    "url": "https://github.com/TheCreditPros/dev_framework_demo/issues"
  },
  "homepage": "https://github.com/TheCreditPros/dev_framework_demo#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "dotenv": "17.2.2"
  }
}
