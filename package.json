{
  "name": "ai-sdlc-framework",
  "version": "2.6.0",
  "description": "AI-Powered SDLC Framework for The Credit Pros",
  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "prepare": "husky",
    "lint": "eslint . --cache --cache-location .eslintcache/",
    "format": "prettier --write .",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:unit": "vitest run tests/unit/",
    "test:changed": "vitest --changed HEAD~1",
    "test:watch-coverage": "vitest --coverage --watch",
    "test:e2e-headed": "playwright test --headed",
    "ci:test-fast": "npm run lint && npm run test:changed",
    "lint:fix": "eslint . --cache --cache-location .eslintcache/ --fix",
    "type-check": "tsc --noEmit",
    "build": "echo 'Build completed successfully'",
    "dev": "echo 'Development server would start here'",
    "validate": "./ai-sdlc validate",
    "repair": "./ai-sdlc repair",
    "docker:up": "./ai-sdlc docker up",
    "docker:down": "./ai-sdlc docker down",
    "docker:logs": "./ai-sdlc docker logs",
    "docker:status": "./ai-sdlc docker status",
    "teams:setup": "./ai-sdlc teams setup",
    "teams:test": "./ai-sdlc teams test",
    "teams:validate": "./ai-sdlc teams validate",
    "perf:monitor": "./ai-sdlc perf monitor",
    "perf:optimize": "./ai-sdlc perf optimize",
    "perf:report": "./ai-sdlc perf report",
    "dev:report": "./ai-sdlc dev report",
    "dev:clean": "./ai-sdlc dev clean",
    "dev:updates": "./ai-sdlc dev updates",
    "ai-sdlc:init": "./ai-sdlc init",
    "ai-sdlc:status": "./ai-sdlc status",
    "ai-sdlc:doctor": "./ai-sdlc doctor",
    "ai-sdlc:serve": "./ai-sdlc serve"
  },
  "keywords": [
    "ai",
    "sdlc",
    "automation",
    "laravel",
    "react",
    "typescript"
  ],
  "author": "Damon DeCrescenzo <damon@thecreditpros.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.54.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.7.0",
    "commitlint": "^19.8.1",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "semantic-release": "^24.2.7",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.php": [
      "bash -c 'if [ -f ./vendor/bin/pint ] && [[ \"$0\" != *\".template.php\" ]]; then ./vendor/bin/pint \"$0\"; fi'"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "express": "^4.18.2",
    "posthog-js": "^1.258.5"
  }
}
