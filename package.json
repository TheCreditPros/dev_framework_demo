{
  "name": "ai-sdlc-framework",
  "version": "3.2.2",
  "description": "AI-Powered SDLC Framework for The Credit Pros",
  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "prepare": "husky",
    "lint": "eslint . --cache --cache-location .eslintcache/",
    "format": "prettier --write .",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:unit": "vitest run tests/unit/",
    "test:changed": "vitest run --changed HEAD~1 --reporter=verbose",
    "test:smart": "node scripts-complex/smart-test-selector.js",
    "test:watch-coverage": "vitest --coverage --watch",
    "test:e2e-headed": "playwright test --headed",
    "ci:test-fast": "npm run lint && npm run test:changed",
    "lint:fix": "eslint . --cache --cache-location .eslintcache/ --fix",
    "type-check": "tsc --noEmit",
    "build": "echo 'Build completed successfully'",
    "dev": "echo 'Development server would start here'",
    "validate": "node validate-setup.js",
    "api:validate-docs": "node scripts-complex/api-documentation-validator.js validate",
    "api:validate-errors": "node scripts-complex/api-error-validator.js validate",
    "api:test-contracts": "node scripts-complex/api-contract-tester.js test",
    "api:analyze-processes": "node scripts-complex/api-process-analyzer.js analyze",
    "api:validate-all": "npm run api:validate-docs && npm run api:validate-errors && npm run api:test-contracts && npm run api:analyze-processes"
  },
  "keywords": [
    "ai",
    "sdlc",
    "automation",
    "laravel",
    "react",
    "typescript"
  ],
  "author": "Damon DeCrescenzo <damon@thecreditpros.com>",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.0.1",
    "dotenv": "^17.2.1",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}