{
  "name": "ai-sdlc-framework",
  "version": "3.3.2",
  "description": "Universal AI-SDLC Framework for any technology stack - AI-powered development automation with quality gates, testing, and intelligent PR reviews",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "node -e \"console.log('Build completed successfully')\" && tsc && vite build",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "lint:ci": "eslint . --max-warnings=0",
    "format": "prettier --write .",
    "format:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,md}'",
    "format:write": "prettier --write .",
    "type-check": "tsc --noEmit",
    "e2e": "playwright test",
    "e2e:ci": "playwright test --project=chromium --reporter=github,line",
    "e2e:debug": "playwright test --debug",
    "e2e:headed": "playwright test --headed",
    "e2e:mobile": "playwright test --project=mobile-chrome",
    "e2e:accessibility": "playwright test --project=accessibility",
    "e2e:ui": "playwright test --ui",
    "e2e:install": "playwright install",
    "e2e:install-deps": "playwright install-deps",
    "e2e:codegen": "playwright codegen",
    "e2e:report": "playwright show-report",
    "e2e:trace": "playwright show-trace",
    "test:all": "npm run test:ci && npm run e2e:ci",
    "test:smoke": "npm run e2e -- --grep @smoke",
    "test:regression": "npm run e2e -- --grep @regression",
    "quality": "npm run lint && npm run format",
    "quality:check": "npm run lint:check && npm run format:check",
    "quality-gates": "./scripts/local-quality-gates.sh",
    "validate-ci": "node scripts/validate-ci-consistency.cjs",
    "pre-push": "npm run validate-ci"
  },
  "keywords": [
    "ai-sdlc",
    "development-automation",
    "quality-gates",
    "ai-testing",
    "pr-agent",
    "multi-stack",
    "universal-framework",
    "development-tools",
    "automation",
    "testing",
    "code-quality"
  ],
  "author": "The Credit Pros Development Team",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "24.3.1",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "5.0.2",
    "@vitest/ui": "3.2.4",
    "axios": "1.12.2",
    "eslint": "^9.35.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "typescript": "5.9.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/your-repo-name.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/your-repo-name/issues"
  },
  "homepage": "https://github.com/your-username/your-repo-name#readme",
  "engines": {
    "node": ">=20"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.50.1"
  },
  "overrides": {
    "cross-spawn": "^7.0.6",
    "esbuild": "^0.25.9",
    "@babel/core": "^7.26.0",
    "@babel/traverse": "^7.26.0",
    "@babel/helper-module-transforms": "^7.26.0",
    "@babel/helper-module-imports": "^7.26.0"
  }
}
